<%= form_with model:[@competition, @photo], html: { multipart: true } do |form| %>
<div class='form-control'>
  <%= form.label "image" %>
  <%= form.file_field :image %>
  <%= form.label "name" %>
  <%= form.text_field :name%>
  <%= form.label "description" %>
  <%= form.text_field :description%>
</div>
     <div class='btn'>
      <%=form.submit "Save", class:'btn btn-lg btn-primary'%>
    </div>
</div>
<% end %>
<div class='btn'>
  <%= link_to 'Back to all competitions', root_path, class:'btn btn-lg btn-primary' %>
</div>